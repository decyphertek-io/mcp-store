{
  "version": "1.0.0",
  "name": "DecypherTek MCP Store",
  "description": "Collection of MCP servers for DecypherTek AI",
  "servers": [
    {
      "id": "cloud-custodian-mcp",
      "name": "Cloud Custodian MCP",
      "description": "AWS compliance scanning and policy enforcement using Cloud Custodian",
      "github_url": "https://github.com/decyphertek/mcp-store/blob/main/servers/cloud-custodian",
      "python_file": "server.py",
      "version": "1.0.0",
      "author": "DecypherTek",
      "requirements": [
        "cloud-custodian>=0.9.30",
        "boto3>=1.28.0"
      ],
      "config_schema": {
        "aws_access_key_id": {
          "type": "string",
          "description": "AWS Access Key ID",
          "required": true
        },
        "aws_secret_access_key": {
          "type": "string",
          "description": "AWS Secret Access Key",
          "required": true,
          "secret": true
        },
        "aws_region": {
          "type": "string",
          "description": "AWS Region",
          "default": "us-east-1"
        }
      },
      "mobile_compatible": true
    },
    {
      "id": "web-search-mcp",
      "name": "Web Search MCP",
      "description": "Web search and scraping capabilities for AI context",
      "github_url": "https://github.com/decyphertek/mcp-store/blob/main/servers/web-search",
      "python_file": "server.py",
      "version": "1.0.0",
      "author": "DecypherTek",
      "requirements": [
        "beautifulsoup4>=4.12.0",
        "requests>=2.31.0",
        "duckduckgo-search>=4.0.0"
      ],
      "config_schema": {
        "search_engine": {
          "type": "string",
          "description": "Search engine to use",
          "enum": ["duckduckgo", "google"],
          "default": "duckduckgo"
        },
        "max_results": {
          "type": "integer",
          "description": "Maximum search results",
          "default": 10
        }
      },
      "mobile_compatible": true
    },
    {
      "id": "file-analyzer-mcp",
      "name": "File Analyzer MCP",
      "description": "Analyze and extract information from various file types",
      "github_url": "https://github.com/decyphertek/mcp-store/blob/main/servers/file-analyzer",
      "python_file": "server.py",
      "version": "1.0.0",
      "author": "DecypherTek",
      "requirements": [
        "PyPDF2>=3.0.0",
        "python-docx>=1.0.0",
        "openpyxl>=3.1.0"
      ],
      "config_schema": {},
      "mobile_compatible": true
    },
    {
      "id": "calendar-mcp",
      "name": "Calendar MCP",
      "description": "Calendar and scheduling integration",
      "github_url": "https://github.com/decyphertek/mcp-store/blob/main/servers/calendar",
      "python_file": "server.py",
      "version": "1.0.0",
      "author": "DecypherTek",
      "requirements": [
        "caldav>=1.3.0",
        "icalendar>=5.0.0"
      ],
      "config_schema": {
        "caldav_url": {
          "type": "string",
          "description": "CalDAV server URL",
          "required": true
        },
        "username": {
          "type": "string",
          "description": "CalDAV username",
          "required": true
        },
        "password": {
          "type": "string",
          "description": "CalDAV password",
          "required": true,
          "secret": true
        }
      },
      "mobile_compatible": true
    }
  ]
}

